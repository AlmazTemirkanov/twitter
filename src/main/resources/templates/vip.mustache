<script>
    function addition() {
        var a = parseFloat(document.getElementById('a').value);
        var b = parseFloat(document.getElementById('b').value);

        if (isNaN(a)==true) a=0;
        if (isNaN(b)==true) b=0;

        var t = a - (b*0.95);
        if (t<0) {
            document.getElementById('result').innerHTML = "Большой кредит доверия";

        }
        else {

            document.getElementById('result').innerHTML = a + " - " + b + "* 0,95 " + " = мин. сумма для активации " + Math.round(t) + " сом ";
        }
    }
</script>


<script>
